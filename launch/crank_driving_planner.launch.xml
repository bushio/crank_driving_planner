<launch>
  
  <arg name="input_crank_driving_trajectory" default="/planning/scenario_planning/lane_driving/motion_planning/obstacle_avoidance_planner/trajectory"/>
  <arg name="output_crank_driving_trajectory" default="/crank_driving_planner/trajectory"/>
  <arg name="input_acceleration" default="/localization/acceleration"/>
  <arg name="input_odometry" default="/localization/kinematic_state"/>
  <arg name="input_velocity_report" default="/vehicle/status/velocity_status"/>
  <arg name="input_motion_state" default="/api/motion/state"/>

  <node pkg="crank_driving_planner" exec="crank_driving_planner_node" name="crank_driving_planner" output="screen">
    <remap from="~/input/trajectory" to="$(var input_crank_driving_trajectory)"/>
    <remap from="~/output/trajectory" to="$(var output_crank_driving_trajectory)"/>
    <remap from="~/input/acceleration" to="$(var input_acceleration)"/>
    <remap from="~/input/odometry" to="$(var input_odometry)"/>
    
  </node>
</launch>